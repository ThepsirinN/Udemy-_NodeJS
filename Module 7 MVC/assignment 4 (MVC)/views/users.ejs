<%- include('layout/header.ejs') -%> 
    <%if (data.length > 0) { %>
        <%for (let i in data) {%>
        <div class="card">
            <div class="user-card">
                <img src="https://icon-library.com/images/default-user-icon/default-user-icon-13.jpg" alt="User_image_profile" class="img_profile">
                <hr class="hr-user">
                <span class="usern"><%= data[i].Username %> </span>
            </div>
        </div>
            <%if (data.length - i != 1){ %> 
                <hr class="hr-end">
            <% }else{ %> 
                <p></p>
            <% } %> 
        <% } %> 
    <%} else { %> 
        <h3> No Profile Insert </h3>
    <% } %> 
<%- include('layout/footer.ejs') -%> 